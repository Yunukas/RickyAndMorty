<Window x:Class="RickyAndMorty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RickyAndMorty"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
    <Grid>
        <Frame x:Name="Main" Content="Frame" NavigationUIVisibility="Hidden" HorizontalAlignment="Left" Height="300" Margin="92,29,0,0" VerticalAlignment="Top" Width="500"/>
        <Button x:Name="species_button" Content="Species" HorizontalAlignment="Left" Margin="10,94,0,0" VerticalAlignment="Top" Width="75" Click="Species_button_Click"/>
        <Button x:Name="charsPage_button" Content="Characters" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" Width="75" Click="CharsPage_button_Click"/>
    </Grid>
</Window>
